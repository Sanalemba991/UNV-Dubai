const structuredData = {
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "Organization",
      "@id": "https://unvbang.netlify.app/#organization",
      "name": "Digital Link Technology LLC",
      "url": "https://unvbang.netlify.app",
      "logo": {
        "@type": "ImageObject",
        "@id": "https://unvbang.netlify.app/unvdubai.png",
        "url": "https://unvbang.netlify.app/unvdubai.png",
        "contentUrl": "https://digitallink.ae/wp-content/uploads/2023/04/imageedit_3_7633242330.png",
        "caption": "unvdubai",
        "inLanguage": "en-US",
        "width": 1000,
        "height": 375
      }
    },
    {
      "@type": "WebSite",
      "@id": "https://unvbang.netlify.app/#website",
      "url": "https://unvbang.netlify.app",
      "name": "unvbang.netlify.app",
      "publisher": {
        "@id": "https://unvbang.netlify.app/#organization"
      },
      "inLanguage": "en-US"
    },
    {
      "@type": "WebPage",
      "@id": "https://unvbang.netlify.app/contact/#webpage",
      "url": "https://unvbang.netlify.app/contact/",
      "name": "Contact UNV Authorized Distributor in Dubai, UAE | Digital Link Technology",
      "description": "Get in touch with Digital Link Technology, the authorized UNV distributor in Dubai and UAE. Contact us for inquiries, support, and professional security solutions.",
      "isPartOf": {
        "@id": "https://unvbang.netlify.app/#website"
      },
      "about": {
        "@id": "https://unvbang.netlify.app/#organization"
      },
      "inLanguage": "en-US"
    },
    {
      "@type": "ContactPage",
      "@id": "https://unvbang.netlify.app/contact/#contactpage",
      "url": "https://unvbang.netlify.app/contact/",
      "name": "Contact UNV Dubai Distributor",
      "description": "Contact page for Digital Link Technology LLC, authorized UNV distributor in Dubai, UAE. Reach out for product inquiries, technical support, and professional security solutions.",
      "isPartOf": {
        "@id": "https://unvbang.netlify.app/contact/#webpage"
      },
      "inLanguage": "en-US"
    }
  ]
};
---

<Layout title={title} description={description} structuredData={structuredData}>
  <main>
    <ContactDigi/>	
  </main>
</Layout>